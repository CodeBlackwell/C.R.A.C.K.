        },
        {
          "name": "<OUTPUT>",
          "description": "Output file",
          "example": "nikto_scan.txt",
          "required": false
        }
      ],
      "flag_explanations": {
        "-h": "Target host/URL to scan",
        "-output": "Save output to file for documentation"
