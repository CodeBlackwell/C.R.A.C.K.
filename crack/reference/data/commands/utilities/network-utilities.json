      "command": "host <DOMAIN>",
      "description": "Simple DNS lookup utility",
      "tags": ["utility", "network", "dns", "recon", "OSCP:HIGH"],
      "variables": [
        {
          "name": "<DOMAIN>",
          "description": "Domain name or IP to query",
          "example": "example.com",
          "required": true
        }
      ],
      "flag_explanations": {
        "-t": "Query specific record type",
        "-a": "All records (equivalent to ANY)"
