{
  "commands": [
    {
      "id": "ping-host",
      "name": "Ping Host",
      "category": "recon",
      "command": "ping -c <COUNT> <TARGET>",
      "description": "Test network connectivity using ICMP echo requests",
      "tags": ["utility", "network", "recon", "OSCP:HIGH"],
      "variables": [
        {
          "name": "<COUNT>",
          "description": "Number of ping packets to send",
          "example": "4",
          "required": false
        },
        {
          "name": "<TARGET>",
          "description": "Target IP or hostname",
          "example": "192.168.1.1",
          "required": true
        }
      ],
      "flag_explanations": {
        "-c": "Count - number of packets to send (default: infinite on Linux)",
        "-i": "Interval between packets in seconds",
        "-W": "Timeout in seconds"
      },
      "success_indicators": ["bytes from", "time="],
      "failure_indicators": ["100% packet loss", "Destination Host Unreachable"],
      "oscp_relevance": "high",
      "notes": "First test for connectivity - note Windows uses -n instead of -c"
    },
    {
      "id": "traceroute-host",
      "name": "Trace Route to Host",
      "category": "recon",
      "command": "traceroute <TARGET>",
      "description": "Trace network path to target showing all hops",
      "tags": ["utility", "network", "recon", "OSCP:MEDIUM"],
      "variables": [
        {
          "name": "<TARGET>",
          "description": "Target IP or hostname",
          "example": "192.168.1.1",
          "required": true
        }
      ],
      "flag_explanations": {
        "-n": "No DNS resolution (faster)",
        "-m": "Maximum TTL hops",
        "-p": "Base port number"
      },
      "alternatives": ["tracert-windows"],
      "oscp_relevance": "medium",
      "notes": "Useful for understanding network topology and firewall positions"
    },
    {
      "id": "tracert-windows",
      "name": "Trace Route (Windows)",
      "category": "recon",
      "command": "tracert <TARGET>",
      "description": "Windows traceroute implementation",
      "tags": ["utility", "network", "windows", "recon", "OSCP:MEDIUM"],
      "variables": [
        {
          "name": "<TARGET>",
          "description": "Target IP or hostname",
          "example": "192.168.1.1",
          "required": true
        }
      ],
      "flag_explanations": {
        "-d": "No DNS resolution",
        "-h": "Maximum hops"
      },
      "alternatives": ["traceroute-host"],
      "oscp_relevance": "medium"
    },
    {
      "id": "dig-dns",
      "name": "DNS Lookup with Dig",
      "category": "recon",
      "command": "dig <DOMAIN> <TYPE>",
      "description": "Perform DNS query using dig",
      "tags": ["utility", "network", "dns", "recon", "OSCP:HIGH"],
      "variables": [
        {
          "name": "<DOMAIN>",
          "description": "Domain name to query",
          "example": "example.com",
          "required": true
        },
        {
          "name": "<TYPE>",
          "description": "Record type (A, AAAA, MX, NS, TXT, ANY)",
          "example": "A",
          "required": false
        }
      ],
      "flag_explanations": {
        "+short": "Short output - just the answer",
        "+noall +answer": "Clean output with only answer section",
        "@<server>": "Query specific DNS server",
        "axfr": "Zone transfer attempt"
      },
      "alternatives": ["nslookup-dns", "host-dns"],
      "oscp_relevance": "high",
      "notes": "Essential for DNS enumeration and zone transfer attempts"
    },
    {
      "id": "nslookup-dns",
      "name": "DNS Query with Nslookup",
      "category": "recon",
      "command": "nslookup <DOMAIN>",
      "description": "Query DNS records using nslookup",
      "tags": ["utility", "network", "dns", "recon", "OSCP:HIGH"],
      "variables": [
        {
          "name": "<DOMAIN>",
          "description": "Domain name to query",
          "example": "example.com",
          "required": true
