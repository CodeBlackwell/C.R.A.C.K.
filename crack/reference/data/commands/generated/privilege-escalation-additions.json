    {
      "id": "suid-find",
      "name": "SUID - Find Binaries",
      "description": "Find SUID/SGID binaries for privilege escalation",
      "command": "find / -perm -u=s -type f 2>/dev/null",
      "category": "privilege-escalation",
      "subcategory": "linux",
      "tags": [
        "oscp",
        "suid",
        "linux",
        "privesc",
        "find"
      ],
      "alternatives": [
        "find / -perm -4000 2>/dev/null (octal notation)"
      ],
      "next_steps": [
        "Check unusual binaries",
        "Search GTFOBins",
        "Test exploits"
      ],
      "flag_explanations": {
        "-perm -u=s": "Find SUID bit set",
        "-type f": "Files only",
        "2>/dev/null": "Suppress errors"
      }
    },
    {
