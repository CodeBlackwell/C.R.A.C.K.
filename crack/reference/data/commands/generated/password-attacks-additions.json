{
  "commands": [
    {
      "id": "hydra-ftp",
      "name": "Hydra - FTP Brute Force",
      "description": "Brute force FTP credentials",
      "command": "hydra -L <USERLIST> -P <PASSLIST> ftp://<TARGET>",
      "category": "password-attacks",
      "subcategory": "brute-force",
      "variables": [
        {
          "name": "<USERLIST>",
          "description": "Username wordlist",
          "example": "/usr/share/wordlists/metasploit/unix_users.txt",
          "required": true
        },
        {
          "name": "<PASSLIST>",
          "description": "Password wordlist",
          "example": "/usr/share/wordlists/rockyou.txt",
          "required": true
        },
        {
          "name": "<TARGET>",
          "description": "Target IP",
          "example": "192.168.1.1",
          "required": true
        }
      ],
      "tags": [
        "oscp",
        "hydra",
        "ftp",
        "brute-force"
      ],
      "next_steps": [
        "Test credentials",
        "Access FTP server",
        "Enumerate files"
      ],
      "flag_explanations": {
        "-L": "Username list",
        "-P": "Password list",
        "-t": "Parallel tasks (default 16)",
        "-V": "Verbose (show attempts)"
      }
    },
    {
      "id": "hydra-http",
      "name": "Hydra - HTTP POST Brute Force",
      "description": "Brute force HTTP login forms",
      "command": "hydra -l <USER> -P <PASSLIST> <TARGET> http-post-form '<PATH>:<PARAMS>:<FAIL_STRING>'",
      "category": "password-attacks",
      "subcategory": "brute-force",
      "variables": [
        {
          "name": "<USER>",
          "description": "Username",
          "example": "admin",
          "required": true
        },
        {
          "name": "<PASSLIST>",
          "description": "Password list",
          "example": "/usr/share/wordlists/rockyou.txt",
          "required": true
        },
        {
          "name": "<TARGET>",
          "description": "Target domain/IP",
          "example": "target.com",
          "required": true
        },
        {
          "name": "<PATH>",
          "description": "Login path",
          "example": "/login.php",
          "required": true
        },
        {
          "name": "<PARAMS>",
          "description": "POST parameters",
          "example": "user=^USER^&pass=^PASS^",
          "required": true
        },
        {
          "name": "<FAIL_STRING>",
          "description": "Failure indicator",
          "example": "Login failed",
          "required": true
        }
      ],
      "tags": [
        "oscp",
        "hydra",
        "http",
        "brute-force",
        "web"
      ],
      "next_steps": [
        "Verify credentials",
        "Access admin panel"
      ],
      "flag_explanations": {
        "-l": "Single username",
        "-L": "Username list",
        "http-post-form": "HTTP POST form module"
      }
    },
