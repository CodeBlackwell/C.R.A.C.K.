        {
          "name": "<HASHFILE>",
          "description": "Hash file",
          "example": "hashes.txt",
          "required": true
        },
        {
          "name": "<WORDLIST>",
          "description": "Wordlist",
          "example": "/usr/share/wordlists/rockyou.txt",
          "required": true
