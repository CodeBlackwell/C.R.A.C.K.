    {
      "id": "hydra-ftp",
      "name": "Hydra - FTP Brute Force",
      "description": "Brute force FTP credentials",
      "command": "hydra -L <USERLIST> -P <PASSLIST> ftp://<TARGET>",
      "category": "password-attacks",
      "subcategory": "brute-force",
      "variables": [
        {
          "name": "<USERLIST>",
          "description": "Username wordlist",
          "example": "/usr/share/wordlists/metasploit/unix_users.txt",
          "required": true
        },
        {
          "name": "<PASSLIST>",
          "description": "Password wordlist",
          "example": "/usr/share/wordlists/rockyou.txt",
          "required": true
        },
        {
          "name": "<TARGET>",
          "description": "Target IP",
          "example": "192.168.1.1",
          "required": true
        }
      ],
      "tags": [
        "PENTEST",
        "hydra",
        "ftp",
        "brute-force"
      ],
      "next_steps": [],
      "flag_explanations": {
        "-L": "Username list",
        "-P": "Password list",
        "-t": "Parallel tasks (default 16)",
        "-V": "Verbose (show attempts)"
      },
      "methodology_guidance": {
        "after_success": "Test credentials Access FTP server Enumerate files"
      },
      "filled_example": "hydra -L /usr/share/wordlists/metasploit/unix_users.txt -P /usr/share/wordlists/rockyou.txt ftp://192.168.1.1"
    },
