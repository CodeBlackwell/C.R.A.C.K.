    {
      "id": "scp-upload",
      "name": "SCP - Upload File",
      "description": "Upload file to remote host via SCP",
      "command": "scp <LOCAL_FILE> <USER>@<TARGET>:<REMOTE_PATH>",
      "category": "file-transfer",
      "subcategory": "scp",
      "variables": [
        {
          "name": "<LOCAL_FILE>",
          "description": "Local file path",
          "example": "exploit.sh",
          "required": true
        },
        {
          "name": "<USER>",
          "description": "SSH username",
          "example": "user",
          "required": true
        },
        {
          "name": "<TARGET>",
          "description": "Target IP",
          "example": "192.168.1.1",
          "required": true
        },
        {
          "name": "<REMOTE_PATH>",
          "description": "Remote path",
          "example": "/tmp/exploit.sh",
          "required": true
        }
      ],
      "tags": [
        "oscp",
        "scp",
        "file-transfer",
        "upload"
      ],
      "prerequisites": [],
      "next_steps": [],
      "flag_explanations": {
        "-P": "Specify SSH port (uppercase P)",
        "-r": "Recursive (for directories)"
      },
      "methodology_guidance": {
        "after_success": "chmod +x on uploaded file Execute"
      },
      "filled_example": "scp exploit.sh user@192.168.1.1:/tmp/exploit.sh"
    },
