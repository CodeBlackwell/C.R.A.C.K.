{
  "commands": [
    {
      "id": "chisel-server",
      "name": "Chisel - Server Mode",
      "description": "Start Chisel server for reverse tunneling",
      "command": "chisel server -p <PORT> --reverse",
      "category": "tunneling",
      "subcategory": "port-forwarding",
      "variables": [
        {
          "name": "<PORT>",
          "description": "Server port",
          "example": "8000",
          "required": true
        }
      ],
      "tags": [
        "PENTEST",
        "chisel",
        "tunneling",
        "pivoting",
        "server"
      ],
      "prerequisites": [],
      "alternatives": [
        "ssh-dynamic-port-forward",
        "ligolo-server"
      ],
      "next_steps": [
        "chisel-client",
        "chisel-socks"
      ],
      "flag_explanations": {
        "-p": "Server listening port",
        "--reverse": "Enable reverse tunneling",
        "--socks5": "Enable SOCKS5 proxy"
      },
      "methodology_guidance": {
        "after_success": "Run chisel client on compromised host Configure proxychains"
      },
      "filled_example": "chisel server -p 8000 --reverse"
    },
    {
      "id": "chisel-client",
      "name": "Chisel - Client Mode",
      "description": "Connect Chisel client for tunneling",
      "command": "chisel client <SERVER_IP>:<PORT> R:<LPORT>:<TARGET>:<RPORT>",
      "category": "tunneling",
      "subcategory": "port-forwarding",
      "variables": [
        {
          "name": "<SERVER_IP>",
          "description": "Attacker IP",
          "example": "10.10.14.1",
          "required": true
        },
        {
          "name": "<PORT>",
          "description": "Server port",
          "example": "8000",
          "required": true
        },
        {
          "name": "<LPORT>",
          "description": "Local port to open",
          "example": "9090",
          "required": true
        },
        {
          "name": "<TARGET>",
          "description": "Target IP in internal network",
          "example": "172.16.1.10",
          "required": true
        },
        {
          "name": "<RPORT>",
          "description": "Remote port",
          "example": "80",
          "required": true
        }
      ],
      "tags": [
        "PENTEST",
        "chisel",
        "tunneling",
        "pivoting",
        "client"
      ],
      "prerequisites": [
        "chisel-server"
      ],
      "alternatives": [
        "ssh-local-forward",
        "plink-remote-forward",
        "ligolo-agent"
      ],
      "next_steps": [
        "proxychains-nmap",
        "nmap-service-scan"
      ],
      "methodology_guidance": {
        "after_success": "Access service at localhost:<LPORT>"
      },
      "filled_example": "chisel client 10.10.14.1:8000 R:9090:172.16.1.10:80"
    },
