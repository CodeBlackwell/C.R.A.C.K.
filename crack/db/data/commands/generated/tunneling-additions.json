    {
      "id": "ssh-local-forward",
      "name": "SSH - Local Port Forwarding",
      "description": "Forward local port through SSH tunnel",
      "command": "ssh -L <LPORT>:<TARGET>:<RPORT> <USER>@<SSH_HOST>",
      "category": "tunneling",
      "subcategory": "ssh-tunneling",
      "variables": [
        {
          "name": "<LPORT>",
          "description": "Local port",
          "example": "8080",
          "required": true
        },
        {
          "name": "<TARGET>",
          "description": "Target IP in internal network",
          "example": "172.16.1.10",
          "required": true
        },
        {
          "name": "<RPORT>",
          "description": "Remote port",
          "example": "80",
          "required": true
        },
        {
          "name": "<USER>",
          "description": "SSH username",
          "example": "user",
          "required": true
        },
        {
          "name": "<SSH_HOST>",
          "description": "SSH server IP",
          "example": "192.168.1.1",
          "required": true
        }
      ],
      "tags": [
        "PENTEST",
        "ssh",
        "tunneling",
        "port-forwarding",
        "local"
      ],
      "prerequisites": [
        "ssh-connect"
      ],
      "alternatives": [
        "chisel-client",
        "ssh-local-port-forward",
        "plink-remote-forward"
      ],
      "next_steps": [
        "proxychains-nmap",
        "nmap-service-scan"
      ],
      "flag_explanations": {
        "-L": "Local port forwarding",
        "-N": "No command execution (tunnel only)",
        "-f": "Background process"
      },
      "methodology_guidance": {
        "after_success": "Access service at localhost:<LPORT>"
      },
      "filled_example": "ssh -L 8080:172.16.1.10:80 user@192.168.1.1"
    },
    {
