{
  "commands": [
    {
      "id": "mkdir-directory",
      "name": "Create Directory",
      "category": "custom",
      "command": "mkdir -p <PATH>",
      "description": "Create directory with parent directories if they don't exist",
      "tags": [
        "utility",
        "filesystem"
      ],
      "variables": [
        {
          "name": "<PATH>",
          "description": "Directory path to create",
          "example": "/tmp/output",
          "required": true
        }
      ],
      "flag_explanations": {
        "-p": "Create parent directories as needed, no error if directory exists"
      },
      "notes": "Essential for organizing enumeration output and creating staging directories",
      "filled_example": "mkdir -p /tmp/output"
    },
    {
      "id": "chmod-executable",
      "name": "Make File Executable",
      "category": "custom",
      "command": "chmod +x <FILE>",
      "description": "Add execute permission to file for all users",
      "tags": [
        "utility",
        "filesystem",
        "permissions"
      ],
      "variables": [
        {
          "name": "<FILE>",
          "description": "File to make executable",
          "example": "exploit.sh",
          "required": true
        }
      ],
      "flag_explanations": {
        "+x": "Add execute permission for user, group, and others"
      },
      "alternatives": [
        "chmod-numeric-permissions"
      ],
      "notes": "Required for running scripts, exploits, and compiled binaries",
      "filled_example": "chmod +x exploit.sh"
    },
    {
      "id": "chmod-numeric-permissions",
      "name": "Set Numeric Permissions",
      "category": "custom",
      "command": "chmod <PERMS> <FILE>",
      "description": "Set file permissions using numeric (octal) notation",
      "tags": [
        "utility",
        "filesystem",
        "permissions"
      ],
      "variables": [
        {
          "name": "<PERMS>",
          "description": "Numeric permissions (e.g., 755, 644, 777)",
          "example": "755",
          "required": true
        },
        {
          "name": "<FILE>",
          "description": "File or directory to modify",
          "example": "script.sh",
          "required": true
        }
      ],
      "flag_explanations": {
        "755": "rwxr-xr-x - Owner: read/write/execute, Others: read/execute",
        "644": "rw-r--r-- - Owner: read/write, Others: read-only",
        "777": "rwxrwxrwx - Full permissions for all (dangerous)"
      },
      "alternatives": [
        "chmod-executable"
      ],
      "notes": "Common permissions:\n755 for executables, 644 for files, 700 for sensitive scripts",
      "filled_example": "chmod 755 script.sh"
    },
    {
      "id": "chmod-writable",
      "name": "Make File Writable",
      "category": "custom",
      "command": "chmod +w <FILE>",
      "description": "Add write permission to file",
      "tags": [
        "utility",
        "filesystem",
        "permissions"
      ],
      "variables": [
        {
          "name": "<FILE>",
          "description": "File to make writable",
          "example": "config.txt",
          "required": true
        }
      ],
      "flag_explanations": {
        "+w": "Add write permission for user, group, and others"
      },
      "alternatives": [
        "chmod-numeric-permissions"
      ],
      "filled_example": "chmod +w config.txt"
    },
    {
      "id": "chown-file",
      "name": "Change File Ownership",
      "category": "custom",
      "command": "chown <USER>:<GROUP> <FILE>",
      "description": "Change file or directory owner and group",
      "tags": [
        "utility",
        "filesystem",
        "permissions"
      ],
      "variables": [
        {
          "name": "<USER>",
          "description": "New owner username",
          "example": "www-data",
          "required": true
        },
        {
          "name": "<GROUP>",
          "description": "New group name",
          "example": "www-data",
          "required": true
        },
        {
          "name": "<FILE>",
          "description": "File or directory to change ownership",
          "example": "/var/www/shell.php",
          "required": true
        }
      ],
      "flag_explanations": {
        "-R": "Recursively change ownership of directories and contents"
      },
      "notes": "Useful after uploading web shells or modifying system files",
      "filled_example": "chown www-data:www-data /var/www/shell.php"
    },
    {
