    {
      "id": "find-files",
      "name": "Find Files by Pattern",
      "category": "enumeration",
      "command": "find <PATH> -name <PATTERN>",
      "description": "Search for files matching pattern in directory tree",
      "tags": [
        "utility",
        "filesystem",
        "enumeration"
      ],
      "variables": [
        {
          "name": "<PATH>",
          "description": "Starting directory for search",
          "example": "/var/www",
          "required": true
        },
        {
          "name": "<PATTERN>",
          "description": "Filename pattern to match (use quotes with wildcards)",
          "example": "\"*.php\"",
          "required": true
        }
      ],
      "flag_explanations": {
        "-name": "Case-sensitive filename match",
        "-iname": "Case-insensitive filename match",
        "-type f": "Find only files (not directories)",
        "-type d": "Find only directories",
        "-perm": "Find files with specific permissions",
        "-user": "Find files owned by user",
        "-size": "Find files by size"
      },
      "alternatives": [],
      "notes": "Essential for privilege escalation - finding SUID binaries, writable files, config files",
      "prerequisites": [],
      "next_steps": [
        "strings-file"
      ],
      "filled_example": "find /var/www -name \"*.php\""
    },
