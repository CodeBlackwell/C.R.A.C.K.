{
  "commands": [
    {
      "id": "sed-replace",
      "name": "Stream Editor Replace",
      "category": "custom",
      "command": "sed 's/<PATTERN>/<REPLACEMENT>/g' <FILE>",
      "description": "Find and replace text in files using stream editor",
      "tags": [
        "utility",
        "text-processing",
        "OSCP:MEDIUM"
      ],
      "variables": [
        {
          "name": "<PATTERN>",
          "description": "Pattern to search for (supports regex)",
          "example": "old_text",
          "required": true
        },
        {
          "name": "<REPLACEMENT>",
          "description": "Replacement text",
          "example": "new_text",
          "required": true
        },
        {
          "name": "<FILE>",
          "description": "File to process",
          "example": "config.txt",
          "required": true
        }
      ],
      "flag_explanations": {
        "-i": "In-place editing (modify file directly)",
        "-e": "Execute multiple expressions",
        "s///g": "Substitute - g means global (all occurrences)"
      },
      "oscp_relevance": "medium",
      "notes": "Useful for modifying exploit payloads or cleaning up output",
      "filled_example": "sed 's/old_text/new_text/g' config.txt"
    },
    {
      "id": "awk-print",
      "name": "AWK Text Processing",
      "category": "custom",
      "command": "awk '{print $<FIELD>}' <FILE>",
      "description": "Extract and process fields from text",
      "tags": [
        "utility",
        "text-processing",
        "OSCP:MEDIUM"
      ],
      "variables": [
        {
          "name": "<FIELD>",
          "description": "Field number to print (1-based)",
          "example": "1",
          "required": true
        },
        {
          "name": "<FILE>",
          "description": "File to process",
          "example": "output.txt",
          "required": true
        }
      ],
      "flag_explanations": {
        "-F": "Field separator (default: whitespace)",
        "NR": "Current line number",
        "NF": "Number of fields in current line"
      },
      "alternatives": [
        "cut-fields"
      ],
      "oscp_relevance": "medium",
      "notes": "Great for extracting specific columns from command output",
      "filled_example": "awk '{print $1}' output.txt"
    },
    {
      "id": "cut-fields",
      "name": "Cut Fields",
      "category": "custom",
      "command": "cut -d '<DELIM>' -f <FIELDS> <FILE>",
      "description": "Extract specific fields from delimited text",
      "tags": [
        "utility",
        "text-processing",
        "OSCP:MEDIUM"
      ],
      "variables": [
        {
          "name": "<DELIM>",
          "description": "Field delimiter character",
          "example": ":",
          "required": true
        },
        {
          "name": "<FIELDS>",
          "description": "Field numbers to extract (e.g., 1, 1-3, 1,3)",
          "example": "1,3",
          "required": true
        },
        {
          "name": "<FILE>",
          "description": "File to process",
          "example": "/etc/passwd",
          "required": true
        }
      ],
      "flag_explanations": {
        "-d": "Delimiter character",
        "-f": "Field numbers",
        "-c": "Cut by character position instead of fields"
      },
      "alternatives": [
        "awk-print"
      ],
      "oscp_relevance": "medium",
      "notes": "Example:\ncut -d ':' -f 1 /etc/passwd extracts usernames",
      "filled_example": "cut -d ':' -f 1,3 /etc/passwd"
    },
    {
      "id": "sort-lines",
      "name": "Sort Lines",
      "category": "custom",
      "command": "sort <FILE>",
      "description": "Sort lines of text alphabetically",
      "tags": [
        "utility",
        "text-processing",
        "OSCP:LOW"
      ],
      "variables": [
        {
          "name": "<FILE>",
          "description": "File to sort",
          "example": "wordlist.txt",
          "required": true
        }
      ],
      "flag_explanations": {
        "-r": "Reverse sort",
        "-n": "Numeric sort",
        "-u": "Unique - remove duplicates",
        "-k": "Sort by specific field"
      },
      "next_steps": [
        "uniq-lines"
      ],
      "oscp_relevance": "low",
      "notes": "Often combined with uniq to remove duplicates",
      "filled_example": "sort wordlist.txt"
    },
    {
      "id": "uniq-lines",
      "name": "Remove Duplicate Lines",
      "category": "custom",
      "command": "uniq <FILE>",
      "description": "Remove adjacent duplicate lines (requires sorted input)",
      "tags": [
        "utility",
        "text-processing",
        "OSCP:LOW"
      ],
      "variables": [
        {
          "name": "<FILE>",
          "description": "File to process (must be sorted)",
          "example": "sorted.txt",
          "required": true
        }
      ],
      "flag_explanations": {
        "-c": "Count occurrences",
        "-d": "Only show duplicates",
        "-u": "Only show unique lines"
      },
      "prerequisites": [
        "sort-lines"
      ],
      "oscp_relevance": "low",
      "notes": "Must sort first: sort file.txt | uniq",
      "filled_example": "uniq sorted.txt"
    },
    {
      "id": "tr-characters",
      "name": "Translate Characters",
      "category": "custom",
      "command": "tr '<SET1>' '<SET2>'",
      "description": "Translate or delete characters from input",
      "tags": [
        "utility",
        "text-processing",
        "OSCP:MEDIUM"
      ],
      "variables": [
        {
          "name": "<SET1>",
          "description": "Characters to replace",
          "example": "a-z",
          "required": true
        },
        {
          "name": "<SET2>",
          "description": "Replacement characters",
          "example": "A-Z",
          "required": true
        }
      ],
      "flag_explanations": {
        "-d": "Delete characters in SET1",
        "-s": "Squeeze repeats - replace multiple occurrences with single",
        "-c": "Complement SET1"
      },
      "oscp_relevance": "medium",
      "notes": "Example:\necho 'hello' | tr 'a-z' 'A-Z' converts to uppercase",
      "filled_example": "tr 'a-z' 'A-Z'"
    },
