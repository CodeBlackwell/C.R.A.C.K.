    {
      "id": "iptables-list-rules",
      "name": "IPTables - List All Rules",
      "description": "Display all iptables firewall rules (Linux)",
      "command": "sudo iptables -L -v -n",
      "category": "post-exploitation",
      "subcategory": "firewall",
      "variables": [],
      "flag_explanations": {
        "-L": "List all rules in selected chain (default: all chains)",
        "-v": "Verbose output (shows packet/byte counters, interface)",
        "-n": "Numeric output (don't resolve hostnames/ports)"
      },
      "success_indicators": [
        "Chain INPUT",
        "Chain FORWARD",
        "Chain OUTPUT"
      ],
      "failure_indicators": [
        "Permission denied",
        "command not found"
      ],
      "next_steps": [],
      "alternatives": [
        "ufw-status",
        "firewalld-list-all"
      ],
      "tags": [
        "linux",
        "firewall",
        "enumeration",
        "iptables",
        "oscp"
      ],
      "notes": "Run with sudo.\n\nUse -t <table> to specify table (filter, nat, mangle, raw)",
      "methodology_guidance": {
        "after_success": "Identify blocked ports Check for DROP/REJECT rules Add rules to allow traffic"
      }
    },
    {
      "id": "iptables-allow-port",
      "name": "IPTables - Allow Inbound Port",
      "description": "Add iptables rule to allow inbound traffic on specific port",
      "command": "sudo iptables -A INPUT -p <PROTOCOL> --dport <PORT> -j ACCEPT",
      "category": "post-exploitation",
      "subcategory": "firewall",
      "variables": [
        {
          "name": "<PROTOCOL>",
          "description": "Protocol (tcp/udp/icmp)",
          "example": "tcp",
          "required": true
        },
        {
          "name": "<PORT>",
          "description": "Port number to allow",
          "example": "4444",
          "required": true
        }
      ],
      "flag_explanations": {
        "-A INPUT": "Append rule to INPUT chain (inbound traffic)",
        "-p": "Protocol to match",
        "--dport": "Destination port to match",
        "-j ACCEPT": "Jump target (ACCEPT allows the packet)"
      },
      "success_indicators": [
        "Rule added successfully",
        "No error output"
      ],
      "failure_indicators": [
        "Permission denied",
        "Bad argument"
      ],
      "prerequisites": [],
      "next_steps": [],
      "alternatives": [
        "ufw-allow-port"
      ],
      "tags": [
        "linux",
        "firewall",
        "manipulation",
        "iptables",
        "oscp"
      ],
      "notes": "Rule is not persistent by default.\n\nUse iptables-save to persist across reboots.",
      "methodology_guidance": {
        "after_success": "Verify rule: sudo iptables -L INPUT -v -n Test connectivity: nc -lvnp <PORT> Make persistent: sudo iptables-save > /etc/iptables/rules.v4"
      },
      "filled_example": "sudo iptables -A INPUT -p tcp --dport 4444 -j ACCEPT"
    },
    {
      "id": "iptables-port-forward",
      "name": "IPTables - Port Forwarding",
      "description": "Forward traffic from one port to another (port forwarding/redirect)",
      "command": "sudo iptables -t nat -A PREROUTING -p <PROTOCOL> --dport <SRC_PORT> -j REDIRECT --to-port <DST_PORT>",
      "category": "post-exploitation",
      "subcategory": "firewall",
      "variables": [
        {
          "name": "<PROTOCOL>",
          "description": "Protocol (tcp/udp)",
          "example": "tcp",
          "required": true
        },
        {
          "name": "<SRC_PORT>",
          "description": "Source port (incoming traffic)",
          "example": "80",
          "required": true
        },
        {
          "name": "<DST_PORT>",
          "description": "Destination port (redirect to)",
          "example": "8080",
          "required": true
        }
      ],
      "flag_explanations": {
        "-t nat": "Specify NAT table (for port forwarding)",
        "-A PREROUTING": "Append to PREROUTING chain (before routing decision)",
        "-p": "Protocol to match",
        "--dport": "Destination port (incoming)",
        "-j REDIRECT": "Redirect packets to different port",
        "--to-port": "Target port for redirection"
      },
      "success_indicators": [
        "Rule added successfully"
      ],
      "failure_indicators": [
        "Permission denied",
        "Bad argument"
      ],
      "prerequisites": [],
      "next_steps": [],
      "tags": [
        "linux",
        "firewall",
        "port-forward",
        "iptables",
        "oscp"
      ],
      "notes": "Useful for bypassing firewall restrictions or redirecting traffic through allowed ports",
      "methodology_guidance": {
        "after_success": "Verify rule: sudo iptables -t nat -L PREROUTING -v -n Test port forward: nc -lvnp <DST_PORT> Connect from remote: nc <TARGET> <SRC_PORT>"
      },
      "filled_example": "sudo iptables -t nat -A PREROUTING -p tcp --dport 80 -j REDIRECT --to-port 8080"
    },
