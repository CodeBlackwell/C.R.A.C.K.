  "commands": [
    {
      "id": "gobuster-dir",
      "name": "Directory Bruteforce",
      "category": "web",
      "command": "gobuster dir -u <URL> -w <WORDLIST> -x php,asp,aspx,html,txt -t <THREADS>",
      "description": "Discover hidden directories and files",
      "tags": [
        "DIRECTORY_ENUMERATION",
        "DISCOVERY",
        "ENUM",
        "ENUMERATION",
        "GOBUSTER",
        "NOISY",
        "OSCP:HIGH",
        "QUICK_WIN",
        "STARTER",
        "WEB"
      ],
      "variables": [
        {
          "name": "<URL>",
          "description": "Target URL",
          "example": "http://192.168.1.100",
          "required": true
        },
        {
          "name": "<WORDLIST>",
          "description": "Path to wordlist",
          "example": "/usr/share/wordlists/dirb/common.txt",
          "required": true
        },
        {
          "name": "<THREADS>",
          "description": "Number of threads",
          "example": "30",
          "required": false
        }
      ],
      "flag_explanations": {
        "dir": "Directory/file bruteforce mode",
        "-u": "Target URL to scan",
        "-w": "Path to wordlist file",
        "-x": "File extensions to search for (comma-separated)",
        "-t": "Number of concurrent threads (default 10)"
      },
      "success_indicators": [
        "Status: 200",
        "Status: 301",
        "Status: 302",
        "Status: 401",
        "Status: 403"
      ],
      "failure_indicators": [
        "Error:",
        "invalid URL",
        "connection refused"
      ],
      "next_steps": [
        "nikto-scan",
        "lfi-test",
        "sqlmap-basic",
        "whatweb-enum",
        "web-manual-enumeration"
      ],
      "alternatives": [
        "dirbuster",
        "wfuzz-params",
        "gobuster-dir-common",
        "gobuster-dir-custom"
      ],
      "prerequisites": [],
      "notes": "Adjust extensions based on technology stack",
      "oscp_relevance": "high",
      "methodology_guidance": {
        "after_success": "Investigate found directories: curl <URL>/<FOUND_DIR> Scan subdirectories: gobuster dir -u <URL>/<DIR> -w <WORDLIST> Try authenticated scan if 401/403: gobuster dir -u <URL> -w <WORDLIST> -c 'Cookie: session=<SESSION_TOKEN>' Check for backup files: gobuster dir -u <URL> -w <WORDLIST> -x bak,old,~"
      },
      "filled_example": "gobuster dir -u http://192.168.1.100 -w /usr/share/wordlists/dirb/common.txt -x php,asp,aspx,html,txt -t 30"
    },
