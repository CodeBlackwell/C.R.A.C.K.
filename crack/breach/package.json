{
  "name": "breach",
  "version": "0.1.0",
  "description": "B.R.E.A.C.H. - Box Reconnaissance, Exploitation & Attack Command Hub",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite --host",
    "dev:debug": "DEBUG=true vite --host",
    "dev:verbose": "DEBUG=true DEBUG_LEVEL=TRACE DEBUG_CATEGORIES=* vite --host",
    "dev:debug:ipc": "DEBUG=true DEBUG_CATEGORIES=IPC vite --host",
    "dev:debug:neo4j": "DEBUG=true DEBUG_CATEGORIES=NEO4J,QUERY vite --host",
    "dev:debug:pty": "DEBUG=true DEBUG_CATEGORIES=PTY,SESSION vite --host",
    "dev:debug:ui": "DEBUG=true DEBUG_CATEGORIES=UI,ACTION,DATA vite --host",
    "dev:debug:errors": "DEBUG=true DEBUG_LEVEL=ERROR vite --host",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "electron:build": "electron-builder",
    "rebuild": "electron-rebuild -f -w node-pty",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@mantine/core": "^7.3.0",
    "@mantine/hooks": "^7.3.0",
    "@tabler/icons-react": "^2.44.0",
    "cytoscape": "^3.28.1",
    "cytoscape-dagre": "^2.5.0",
    "neo4j-driver": "^5.28.2",
    "node-pty": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-rebuild": "^3.2.9",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.2",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^2.1.0"
  },
  "build": {
    "appId": "com.crack.breach",
    "productName": "BREACH",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist-electron",
      "dist"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development"
    }
  }
}
