  "commands": [
    {
      "id": "apt-install",
      "name": "Install Package with APT",
      "category": "custom",
      "command": "sudo apt install -y <PACKAGE>",
      "description": "Install Debian/Ubuntu package using apt package manager",
      "tags": ["utility", "package-manager", "linux", "OSCP:HIGH"],
      "variables": [
        {
          "name": "<PACKAGE>",
          "description": "Package name to install",
          "example": "nmap",
          "required": true
        }
      ],
      "flag_explanations": {
        "-y": "Automatic yes to prompts",
        "--no-install-recommends": "Don't install recommended packages (minimal install)"
      },
      "prerequisites": ["apt-update"],
      "oscp_relevance": "high",
      "notes": "Essential for installing tools on Debian-based systems (Kali, Ubuntu, Parrot)"
    },
