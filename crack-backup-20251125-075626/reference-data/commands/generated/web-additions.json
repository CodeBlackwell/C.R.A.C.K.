{
  "commands": [
    {
      "id": "ffuf-dir-fuzz",
      "name": "Ffuf - Directory Fuzzing",
      "description": "Fast web fuzzer for directory discovery",
      "command": "ffuf -u http://<TARGET>/FUZZ -w <WORDLIST>",
      "category": "web",
      "subcategory": "fuzzing",
      "variables": [
        {
          "name": "<TARGET>",
          "description": "Target IP/domain",
          "example": "192.168.1.1",
          "required": true
        },
        {
          "name": "<WORDLIST>",
          "description": "Directory wordlist",
          "example": "/usr/share/wordlists/dirb/common.txt",
          "required": true
        }
      ],
      "tags": [
        "oscp",
        "web",
        "fuzzing",
        "directory"
      ],
      "alternatives": [
        "gobuster-dir-basic",
        "wfuzz-dir"
      ],
      "next_steps": [
        "Inspect discovered directories",
        "Check for upload functionality"
      ],
      "flag_explanations": {
        "-u": "Target URL (FUZZ keyword for injection point)",
        "-w": "Wordlist file",
        "-fc": "Filter HTTP status codes",
        "-fs": "Filter response size"
      }
    },
    {
      "id": "ffuf-vhost-fuzz",
      "name": "Ffuf - Virtual Host Discovery",
      "description": "Fuzz for virtual hosts on a target",
      "command": "ffuf -u http://<TARGET> -H 'Host: FUZZ.<DOMAIN>' -w <WORDLIST>",
      "category": "web",
      "subcategory": "fuzzing",
      "variables": [
        {
          "name": "<TARGET>",
          "description": "Target IP",
          "example": "192.168.1.1",
          "required": true
        },
        {
          "name": "<DOMAIN>",
          "description": "Base domain",
          "example": "example.com",
          "required": true
        },
        {
          "name": "<WORDLIST>",
          "description": "Subdomain wordlist",
          "example": "/usr/share/seclists/Discovery/DNS/subdomains-top1million-5000.txt",
          "required": true
        }
      ],
      "tags": [
        "oscp",
        "web",
        "vhost",
        "fuzzing"
      ],
      "next_steps": [
        "Add discovered vhosts to /etc/hosts",
        "Enumerate each vhost"
      ],
      "flag_explanations": {
        "-H": "Custom header (Host header here)",
        "-fs": "Filter by response size"
      }
    },
    {
      "id": "ffuf-param-fuzz",
      "name": "Ffuf - Parameter Fuzzing",
      "description": "Discover hidden GET/POST parameters",
      "command": "ffuf -u 'http://<TARGET>/<PATH>?FUZZ=test' -w <WORDLIST>",
      "category": "web",
      "subcategory": "fuzzing",
      "variables": [
        {
          "name": "<TARGET>",
          "description": "Target domain",
          "example": "target.com",
          "required": true
        },
        {
          "name": "<PATH>",
          "description": "Target path",
          "example": "index.php",
          "required": true
        },
        {
          "name": "<WORDLIST>",
          "description": "Parameter wordlist",
          "example": "/usr/share/seclists/Discovery/Web-Content/burp-parameter-names.txt",
          "required": true
        }
      ],
      "tags": [
        "oscp",
        "web",
        "parameters",
        "fuzzing"
      ],
      "next_steps": [
        "Test discovered parameters for injection"
      ],
      "flag_explanations": {
        "-X": "HTTP method (POST for POST params)",
        "-d": "POST data (use with -X POST)"
      }
    },
